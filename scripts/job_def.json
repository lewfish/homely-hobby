        "image": "279682201306.dkr.ecr.us-east-1.amazonaws.com/raster-vision-gpu-lewfish:pytorch",
        "vcpus": 8,
        "memory": 55000,
        "command": [],
        "volumes": [
            {
                "host": {
                    "sourcePath": "/home/ec2-user"
